<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cindiemarren.wine_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WineActivity" />
        <activity android:name=".FavoritesActivity" />
        <activity android:name=".MapActivity" />
        <activity android:name=".FaqActivity" />
        <activity android:name=".EventsActivity" />
        <activity android:name=".TicketsActivity" />
        <activity android:name=".ActivityTwo" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".WhiteActivity" />
        <activity android:name=".RedActivity" />
        <activity android:name=".SparklingActivity" />
        <activity android:name=".OtherActivity" />
    </application>

</manifest>